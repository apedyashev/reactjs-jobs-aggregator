{
  "name": "redux-webpack-es6-boilerplate",
  "description": "A starter project for modern React apps with Redux",
  "version": "0.7.1",
  "author": "Nick S. Plekhanov <nick.plekhanov@gmail.com>",
  "license": "MIT",
  "keywords": [
    "react",
    "redux",
    "es6",
    "hmr",
    "hot",
    "babel",
    "webpack",
    "boilerplate",
    "starter"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nicksp/redux-webpack-es6-boilerplate.git"
  },
  "bugs": "https://github.com/nicksp/redux-webpack-es6-boilerplate/issues",
  "scripts": {
    "precommit": "npm test",
    "prepush": "npm test",
    "pretest": "eslint ./src --ext .js",
    "clean:build": "node ./bin/clean.js createdir",
    "build:html": "node ./bin/buildHtml.js",
    "prestart": "node ./bin/clean.js",
    "start": "webpack-dev-server --config ./config/webpack.config.dev.js --hot --inline --progress",
    "prebuild": "npm run clean:build && npm run build:html",
    "build": "export NODE_ENV=production & NODE_ENV=production & webpack -p --config ./config/webpack.config.prod.js --progress",
    "lint": "eslint ./src --ext .js",
    "jscs": "jscs src/js/",
    "test": "NODE_PATH=src ava",
    "test:watch": "npm run test -- --watch"
  },
  "devDependencies": {
    "ava": "0.16.0",
    "babel-core": "6.7.7",
    "babel-eslint": "6.0.3",
    "babel-loader": "6.2.4",
    "babel-polyfill": "6.13.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "babel-preset-stage-1": "6.5.0",
    "chai": "3.5.0",
    "chai-jquery": "2.0.0",
    "cheerio": "0.20.0",
    "colors": "1.1.2",
    "css-loader": "0.23.1",
    "enzyme": "2.4.1",
    "eslint": "3.3.0",
    "eslint-config-airbnb": "10.0.1",
    "eslint-plugin-import": "1.13.0",
    "eslint-plugin-jsx-a11y": "2.1.0",
    "eslint-plugin-react": "6.0.0",
    "file-loader": "0.8.5",
    "husky": "0.11.6",
    "jquery": "2.2.2",
    "jscs": "2.10.1",
    "jsdom": "8.2.0",
    "mocha": "2.4.5",
    "node-sass": "3.7.0",
    "postcss-loader": "0.8.2",
    "react-addons-test-utils": "15.0.1",
    "react-hot-loader": "1.3.0",
    "redux-devtools": "3.3.1",
    "redux-devtools-dock-monitor": "1.1.1",
    "redux-devtools-log-monitor": "1.0.11",
    "redux-immutable-state-invariant": "1.2.3",
    "redux-mock-store": "1.1.2",
    "redux-slider-monitor": "1.0.5",
    "rimraf": "2.5.2",
    "sass-loader": "3.1.2",
    "style-loader": "0.13.0",
    "url-loader": "0.5.7",
    "webpack-dev-server": "1.14.1"
  },
  "dependencies": {
    "autoprefixer": "6.3.6",
    "classnames": "2.2.3",
    "copy-webpack-plugin": "2.1.3",
    "extract-text-webpack-plugin": "1.0.1",
    "history": "2.1.0",
    "humps": "1.1.0",
    "lodash": "4.11.1",
    "lost": "7.0.4",
    "material-ui": "0.15.4",
    "normalize.css": "4.2.0",
    "normalizr": "2.2.1",
    "postcss-nested": "1.0.0",
    "qs": "6.2.1",
    "react": "15.3.0",
    "react-dom": "15.3.0",
    "react-icons": "2.2.1",
    "react-redux": "4.4.5",
    "react-router": "2.6.1",
    "react-router-redux": "4.0.5",
    "react-tap-event-plugin": "1.0.0",
    "redux": "3.5.2",
    "redux-logger": "2.6.1",
    "redux-saga": "0.11.0",
    "webpack": "1.13.0",
    "whatwg-fetch": "1.0.0"
  },
  "ava": {
    "files": [
      "src/**/*.spec.js"
    ],
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
